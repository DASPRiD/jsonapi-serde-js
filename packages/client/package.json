{
    "name": "@jsonapi-serde/client",
    "version": "1.1.0",
    "description": "Client side JSON:API deserialization",
    "type": "module",
    "author": "Ben Scholzen 'DASPRiD'",
    "license": "BSD-3-Clause",
    "keywords": [
        "jsonapi",
        "typescript",
        "zod",
        "deserializer",
        "parser"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dasprid/jsonapi-serde-js.git"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "typecheck": "tsc --noEmit",
        "test": "tsx --test",
        "bench-deserializer-types": "tsc -p tsconfig.bench.json",
        "ci:test": "c8 --reporter=lcov pnpm test"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "types": [
                "./dist/index.d.ts",
                "./src/index.ts"
            ],
            "jsonapi-serde": "./src/index.ts",
            "default": "./dist/index.js"
        }
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "expect-type": "^1.2.2",
        "zod": "^4.1.1"
    },
    "peerDependencies": {
        "zod": "^3.25.42 || ^4.0.0"
    }
}
